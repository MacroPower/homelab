    [...str]: any
    affinity?: str
    annotations?: ValuesFilerAnnotations
    containerSecurityContext?: ValuesFilerContainerSecurityContext
    data?: ValuesFilerData
    dataCenter?: any
    defaultReplicaPlacement?: str
    dirListLimit?: int
    disableDirListing?: bool
    disableHttp?: bool
    enablePVC?: bool
    enabled?: bool
    encryptVolumeData?: bool
    extraArgs?: [any]
    extraEnvironmentVars?: ValuesFilerExtraEnvironmentVars
    extraVolumeMounts?: str
    extraVolumes?: str
    filerGroup?: str
    grpcPort?: int
    imageOverride?: any
    ingress?: ValuesFilerIngress
    initContainers?: str
    ipBind?: str
    livenessProbe?: ValuesFilerLivenessProbe
    loggingOverrideLevel?: any
    logs?: ValuesFilerLogs
    maxMB?: any
    metricsIp?: str
    metricsPort?: int
    nodeSelector?: str
    notificationConfig?: str
    podAnnotations?: ValuesFilerPodAnnotations
    podLabels?: ValuesFilerPodLabels
    podManagementPolicy?: str
    podSecurityContext?: ValuesFilerPodSecurityContext
    port?: int
    priorityClassName?: str
    rack?: any
    readinessProbe?: ValuesFilerReadinessProbe
    redirectOnRead?: bool
    replicas?: int
    resources?: ValuesFilerResources
    restartPolicy?: any
