    annotations : ValuesFilerAnnotations, optional
        Annotations to be added to the filer resource
    containerSecurityContext : ValuesFilerContainerSecurityContext, optional
        Configure security context for Container
        ref: https://kubernetes.io/docs/tasks/configure-pod-container/security-context/
        Example:
        containerSecurityContext:
          enabled: true
          runAsUser: 2000
          allowPrivilegeEscalation: false
    data : ValuesFilerData, optional
        You may use ANY storage-class, example with local-path-provisioner
        Annotations are optional.
         data:
           type: "persistentVolumeClaim"
           size: "24Ti"
           storageClass: "local-path-provisioner"
           annotations:
             "key": "value"

        You may also specify an existing claim:
         data:
           type: "existingClaim"
           claimName: "my-pvc"

        You can also use emptyDir storage:
         data:
           type: "emptyDir"
    dataCenter : any, optional, default is "null"
        prefer to read and write to volumes in this data center (not set by default)
    defaultReplicaPlacement : str, optional, default is "000"
         replication type is XYZ:
        X number of replica in other data centers
        Y number of replica in other racks in the same data center
        Z number of replica in other servers in the same rack
    dirListLimit : int, optional, default is 100000
        Limit sub dir listing size (default 100000)
    disableDirListing : bool, optional, default is False
        turn off directory listing
    disableHttp : bool, optional, default is False
        Disable http request, only gRpc operations are allowed
    enablePVC : bool, optional, default is False
        Settings for configuring stateful storage of filer pods.
        enablePVC will create a pvc for filer for data persistence.
    enabled : bool, optional, default is True
    encryptVolumeData : bool, optional, default is False
        encrypt data on volume servers
    extraArgs : [any], optional
        Custom command line arguments to add to the filer command
        Example to fix IPv6 metrics connectivity issues:
        extraArgs: ["-metricsIp", "0.0.0.0"]
        Example with multiple args:
        extraArgs: ["-customFlag", "value", "-anotherFlag"]
    extraEnvironmentVars : ValuesFilerExtraEnvironmentVars, optional
        extraEnvVars is a list of extra environment variables to set with the stateful set.
    extraVolumeMounts : str, optional, default is ""
    extraVolumes : str, optional, default is ""
    filerGroup : str, optional, default is ""
    grpcPort : int, optional, default is 18888
    imageOverride : any, optional, default is "null"
    ingress : ValuesFilerIngress, optional
    initContainers : str, optional, default is ""
    ipBind : str, optional, default is "0.0.0.0"
    livenessProbe : ValuesFilerLivenessProbe, optional
        used to configure livenessProbe on filer containers

    loggingOverrideLevel : any, optional, default is "null"
    logs : ValuesFilerLogs, optional
        You can also use emptyDir storage:
         logs:
           type: "emptyDir"
    maxMB : any, optional, default is "null"
        split files larger than the limit, default 32
    metricsIp : str, optional, default is ""
    metricsPort : int, optional, default is 9327
    nodeSelector : str, optional, default is ""
        nodeSelector labels for server pod assignment, formatted as a muli-line string.
        ref: https://kubernetes.io/docs/concepts/configuration/assign-pod-node/#nodeselector
        Example:
    notificationConfig : str, optional, default is ""
        Add a custom notification.toml to configure filer notifications
        Example:
        notificationConfig: |-
          [notification.kafka]
          enabled = false
          hosts = [
              "localhost:9092"
          ]
          topic = "seaweedfs_filer"
          offsetFile = "./last.offset"
          offsetSaveIntervalSeconds = 10
    podAnnotations : ValuesFilerPodAnnotations, optional
        Annotations to be added to the filer pods
    podLabels : ValuesFilerPodLabels, optional
        Labels to be added to the filer pods
    podManagementPolicy : str, optional, default is "Parallel"
        # Set podManagementPolicy
    podSecurityContext : ValuesFilerPodSecurityContext, optional
        Configure security context for Pod
        ref: https://kubernetes.io/docs/tasks/configure-pod-container/security-context/
        Example:
        podSecurityContext:
          enabled: true
          runAsUser: 1000
          runAsGroup: 3000
          fsGroup: 2000
    port : int, optional, default is 8888
    priorityClassName : str, optional, default is ""
        used to assign priority to server pods
        ref: https://kubernetes.io/docs/concepts/configuration/pod-priority-preemption/
    rack : any, optional, default is "null"
        prefer to write to volumes in this rack (not set by default)
    readinessProbe : ValuesFilerReadinessProbe, optional
        used to configure readinessProbe on filer containers

    redirectOnRead : bool, optional, default is False
        Whether proxy or redirect to volume server during file GET request
    replicas : int, optional, default is 1
    resources : ValuesFilerResources, optional
        Resource requests, limits, etc. for the server cluster placement. This
        should map directly to the value of the resources field for a PodSpec,
        formatted as a multi-line string. By default no direct resource request
        is made.
    restartPolicy : any, optional, default is "null"
    s3 : ValuesFilerS3, optional
    secretExtraEnvironmentVars : ValuesFilerSecretExtraEnvironmentVars, optional
        secret env variables
    serviceAccountName : str, optional, default is ""
        used to assign a service account.
        ref: https://kubernetes.io/docs/tasks/configure-pod-container/configure-service-account/
    sidecars : [any], optional
        #
        # e.g:
        # sidecars:
        #   - name: your-image-name
        #     image: your-image
        #     imagePullPolicy: Always
        #     ports:
        #       - name: portname
        #         containerPort: 1234
        #
    storage : str, optional, default is "25Gi"
        storage should be set to the disk size of the attached volume.
    storageClass : any, optional, default is "null"
        storageClass is the class of storage which defaults to null (the Kube cluster will pick the default).
    tolerations : str, optional, default is ""
        Toleration Settings for server pods
        This should be a multi-line string matching the Toleration array
        in a PodSpec.
    topologySpreadConstraints : str, optional, default is ""
        Topology Spread Constraints Settings
        This should map directly to the value of the topologySpreadConstraints
