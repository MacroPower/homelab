schema ValuesEnterprise:
    r"""
    Configuration for running Enterprise Loki

    Attributes
    ----------
    adminApi : ValuesEnterpriseAdminApi, optional
        If enabled, the correct admin_client storage will be configured. If disabled while running enterprise,
        make sure auth is set to `type: trust`, or that `auth_enabled` is set to `false`.
    adminToken : ValuesEnterpriseAdminToken, optional
        Admin token secret for provisioner (must be created externally)
    canarySecret : any, optional, default is "null"
        Alternative name of the secret to store token for the canary
    cluster_name : any, optional, default is "null"
        Optional name of the GEL cluster, otherwise will use .Release.Name
        The cluster name must match what is in your GEL license
    config : str, optional, default is "{{- if .Values.enterprise.adminApi.enabled }}\nadmin_client:\n  {{ include \"enterprise-logs.adminAPIStorageConfig\" . | nindent 2 }}\n{{ end }}\nauth:\n  type: {{ .Values.enterprise.adminApi.enabled | ternary \"enterprise\" \"trust\" }}\nauth_enabled: {{ .Values.loki.auth_enabled }}\ncluster_name: {{ include \"loki.clusterName\" . }}\nlicense:\n  path: /etc/loki/license/license.jwt\n"
        enterprise specific sections of the config.yaml file
    enabled : bool, optional, default is False
        Enable enterprise features, license must be provided
    externalConfigName : str, optional, default is ""
        Name of the external config secret to use
    externalLicenseName : any, optional, default is "null"
        Name of external license secret to use
    gelGateway : bool, optional, default is True
        Use GEL gateway, if false will use the default nginx gateway
    image : ValuesEnterpriseImage, optional
    license : ValuesEnterpriseLicense, optional
