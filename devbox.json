{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.11.0/.schema/devbox.schema.json",
  "packages": {
    "github:budimanjojo/talhelper/v3.0.41": "",
    "git+https://nur.jacobcolvin.com#kat": "",
    "git+https://nur.jacobcolvin.com#kclipper": "",
    "git+https://nur.jacobcolvin.com#kcl-lsp": "",
    "argocd": "3.1.9",
    "cilium-cli": "0.19.0",
    "doppler": "3.75.2",
    "go-task": "3.45.5",
    "git": "2.51.2",
    "kubeconform": "0.7.0",
    "nodePackages.prettier": "3.5.3",
    "talosctl": "1.11.6",
    "terraform": "1.13.5",
    "opentofu": "1.11.4",
    "spacectl": "1.18.0",
    "natscli": "0.3.1",
    "kubernetes-helm": "3.19.1",
    "auth0-cli": "1.23.0",
    "go": "1.25.5",
    "wrangler": "4.61.0"
  },
  "env": {
    "KCL_LIB_HOME": "${DEVBOX_PROJECT_ROOT}/.devbox/kcl/lib",
    "KCL_PKG_PATH": "${DEVBOX_PROJECT_ROOT}/.devbox/kcl/kpm",
    "KCL_CACHE_PATH": "${DEVBOX_PROJECT_ROOT}/.devbox/kcl/cache"
  },
  "shell": {
    "init_hook": [
      "mkdir -p $KCL_LIB_HOME",
      "mkdir -p $KCL_PKG_PATH",
      "mkdir -p $KCL_CACHE_PATH",
      "echo 'Welcome to devbox!' > /dev/null"
    ]
  }
}
